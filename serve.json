{
  "headers": [
    {
      "source": "**/**.html",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, no-cache"
      }]
    },
    {
      "source": "static/**",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=31536000, immutable"
      }]
    }
  ]
}
